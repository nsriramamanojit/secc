  <div class="desc"><b>Reference Name</b> : <%= @panchayat.reference_name %> </div>
  <div class="divider"></div>
  <div class="desc"><b>State Name</b> :  <%= @panchayat.state.name %></div>
  <div class="divider"></div>
  <div class="desc"><b>District Name</b> :  <%= @panchayat.district.name %></div>
  <div class="divider"></div>
  <div class="desc"><b>Revenue Block Name</b> :  <%= @panchayat.revenue_block.name %></div>
