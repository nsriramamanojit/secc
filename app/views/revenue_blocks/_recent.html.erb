<div class="recent_items">
  <div class="margin">
    <h3>Recent Revenue Blocks</h3>
    <% if @recent.blank? %>
        No Blocks Added Yet...
    <% else %>
        <% for rb in @recent %>
            <div id="margin">
              <div class="left">&nbsp;</div>
              <div class="right">&nbsp;</div>
              <div class="left-to-right">
                <div class="pen">&nbsp;</div>
                <div class="text">
                  <%= link_to rb.name, revenue_block_path(rb), :class=>'usuallink' %>
                  <div style="margin: 8px 0px;">&nbsp;</div>
                </div>
              </div>
            </div>
        <% end %>
    <% end %>
    <div class="see_all">
      <a href="/revenue_blocks">
        View all Blocks
        <img hspace="2" border="0" alt="" src="/images/blue^.gif">
      </a>
    </div>
  </div>
</div>