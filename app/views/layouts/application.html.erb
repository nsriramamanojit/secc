							<div class="margin">
								<h3>Recent Records (ताज़ा)</h3>
								<div class="panel">
								<%= render :partial=>'recent_items' %>
								</div>
							</div>
						</div>
						</td>
						<td width="5"> &nbsp;</td>
						<td valign="top">
						<div class="statistic">
							<div class="margin">
								<div id="flash_messages">
								<%= render :partial=>'common/import_flash' %>
